{
	"name": "cheesa-reference",
	"version": "0.0.0",
	"description": "Recommendation system made easy",
	"main": "dist/index.js",
	"scripts": {
		"lint": "npx @biomejs/biome lint --files-ignore-unknown=true src",
		"format": "npx @biomejs/biome format src --write --json-formatter-enabled=true",
		"dev": "nodemon src/index.ts",
		"test": "vitest",
		"build": "tsup",
		"local:build": "rm -rf dist && npm run build && npm start",
		"start": "node dist/index.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run format"
		}
	},
	"author": "Intelitech",
	"license": "ISC",
	"dependencies": {
		"@types/nodemailer": "^6.4.14",
		"bcryptjs": "^2.4.3",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.0.3",
		"nodemailer": "^6.9.8",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.4.1",
		"@swc/core": "^1.3.100",
		"@swc/helpers": "^0.5.3",
		"@types/bcryptjs": "^2.4.6",
		"@types/cookie-parser": "^1.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/node": "^20.10.4",
		"husky": "^8.0.3",
		"nodemon": "^3.0.2",
		"regenerator-runtime": "^0.14.0",
		"ts-node": "^10.9.1",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "^1.0.2"
	}
}
